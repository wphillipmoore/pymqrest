* pymqrest seed objects for QM1

DEFINE QLOCAL(PYMQREST.DEAD.LETTER) REPLACE DEFPSIST(YES) DESCR('pymqrest dead letter')
ALTER QMGR DEADQ(PYMQREST.DEAD.LETTER)

DEFINE QLOCAL(PYMQREST.QLOCAL) REPLACE DEFPSIST(YES) DEFPRESP(SYNC) DESCR('pymqrest local queue')
DEFINE QLOCAL(PYMQREST.XMITQ) REPLACE DEFPSIST(YES) USAGE(XMITQ) DESCR('pymqrest xmit queue')
DEFINE QREMOTE(PYMQREST.QREMOTE) REPLACE RNAME('PYMQREST.TARGET') RQMNAME('QM1') XMITQ(PYMQREST.XMITQ) DESCR('pymqrest remote queue')
DEFINE QALIAS(PYMQREST.QALIAS) REPLACE TARGQ(PYMQREST.QLOCAL) DESCR('pymqrest alias queue')
DEFINE QMODEL(PYMQREST.QMODEL) REPLACE DEFTYPE(TEMPDYN) DEFSOPT(SHARED) DESCR('pymqrest model queue')

DEFINE TOPIC(PYMQREST.TOPIC) REPLACE TOPICSTR('pymqrest/topic') DESCR('pymqrest topic')
DEFINE NAMELIST(PYMQREST.NAMELIST) REPLACE NAMES(PYMQREST.QLOCAL) DESCR('pymqrest namelist')

DEFINE CHANNEL(PYMQREST.SVRCONN) CHLTYPE(SVRCONN) TRPTYPE(TCP) REPLACE DESCR('pymqrest svrconn')
DEFINE CHANNEL(PYMQREST.SDR) CHLTYPE(SDR) TRPTYPE(TCP) XMITQ(PYMQREST.XMITQ) CONNAME('localhost(1414)') REPLACE DESCR('pymqrest sdr')
DEFINE CHANNEL(PYMQREST.RCVR) CHLTYPE(RCVR) TRPTYPE(TCP) REPLACE DESCR('pymqrest rcvr')

DEFINE LISTENER(PYMQREST.LSTR) TRPTYPE(TCP) PORT(1415) CONTROL(QMGR) REPLACE DESCR('pymqrest listener')
DEFINE PROCESS(PYMQREST.PROC) REPLACE APPLICID('/bin/true') DESCR('pymqrest process')

* Cross-QM objects for multi-queue-manager examples

DEFINE QLOCAL(PYMQREST.QM2.XMITQ) REPLACE DEFPSIST(YES) USAGE(XMITQ) DESCR('xmit queue to QM2')
DEFINE QREMOTE(PYMQREST.REMOTE.TO.QM2) REPLACE RNAME('PYMQREST.QLOCAL') RQMNAME('QM2') XMITQ(PYMQREST.QM2.XMITQ) DESCR('remote queue to QM2')
DEFINE CHANNEL(QM1.TO.QM2) CHLTYPE(SDR) TRPTYPE(TCP) XMITQ(PYMQREST.QM2.XMITQ) CONNAME('qm2(1414)') REPLACE DESCR('sender to QM2')
DEFINE CHANNEL(QM2.TO.QM1) CHLTYPE(RCVR) TRPTYPE(TCP) REPLACE DESCR('receiver from QM2')

* Disable connection authentication and channel authentication for
* containerized testing so that inter-QM channels can connect without
* complex security setup.

ALTER QMGR CONNAUTH(' ') CHLAUTH(DISABLED)
REFRESH SECURITY TYPE(CONNAUTH)

